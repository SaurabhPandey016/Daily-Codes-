#include <iostream>

using namespace std;

 

int main() {

    //Write your code here

    int n,e=0,o=0,ld;

    cin>>n;

    

    while (n > 0) {

        ld=n%10;

      if (ld % 2 == 0)

        e = e + ld;

      else

        o = o + ld;

      n=n/10;

    }

    cout<<e<<" "<<o;

    return 0;

}
