string morningWalk(int a,int b,int c,int d){

    // Write your code here.

    int cnt=0;

    int p=min(c,d);

    int q=max(c,d);

    int l=min(a,b);

    int r=max(a,b);

    for(int i=l;i<=r;i++)

    {

        if(i>=p && i<=q)

        {

            cnt++;

        }

    }

    if(cnt==0)

    {

        return "Never";

    }

    else if(cnt==1)

    {

        return "Point";

    }

    else

    {

        return "Intersect";

    }

}
