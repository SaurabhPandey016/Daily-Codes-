#include<stack>
#include<string>
#include<algorithm>
#include<iomanip>
using namespace std;


int main()
{
  int t;
  
  cin>>t;
  
  while(t--)
  {
     long long a=103993;
  long long b=33102;
      
      int n;
      cin>>n;
      if(n==0)
      cout<<3;
      
      else{
          cout<<"3.";
      
      while(n--){
      a=10*(a%b);
     
      cout<<a/b;
      }
      }
      cout<<endl;
  }
     
  
}
