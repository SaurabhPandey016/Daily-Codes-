 int kthElement(int k, vector<int>& arr1, vector<int>& arr2) {
      vector<int>combined;
      for(int i=0;i<arr1.size();i++)
      {
          combined.push_back(arr1[i]);
      }
      for(int i=0;i<arr2.size();i++)
      {
          combined.push_back(arr2[i]);
      }
      sort(combined.begin(),combined.end());
      return combined[k - 1];
    }
