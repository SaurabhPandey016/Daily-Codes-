#include <iostream>
using namespace std;

int main() {
	// your code goes here
	int t; cin>>t;
	while(t--){
	    int n; cin>>n;
	    int a[n];
	    int k=0;
	    for(int i=0;i<n;i++){
	        cin>>a[i];
	        if(a[i]==1) k++;
	    }
	    int c = n-k;
	    if(k>=c) cout<<1<<endl;
	    else cout<<0<<endl;
	}
	return 0;
}
