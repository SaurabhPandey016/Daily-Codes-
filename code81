co#include <iostream>
using namespace std;

int main() {
	int t;
	cin >> t;
	while (t--){
        int n;
        cin >> n;
        long long a[n];
        
        long long sum=0;
        for (int i=0; i<n; i++){
            cin >> a[i];
            sum += a[i];
        }
        
        for (int i=0; i<n; i++){
            cout << (sum/(n-1)) - a[i] << " ";
        }
        
        cout << "\n";
	}
	return 0;
}
