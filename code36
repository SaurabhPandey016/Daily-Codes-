#include <bits/stdc++.h>
using namespace std;
#define ll long long int 
int main() {
	// your code goes here

	    ll n;
	    cin>>n;
	    ll a[n];
	   for(int i=0;i<n;i++){
	      cin>>a[i]; 
	   }
	   sort(a,a+n);
	   ll sum=0;
	   for(int i=0;i<n-1;i++){
	       sum = sum+(a[i]*a[i+1]);
	   }
	   cout<<sum<<endl;
		return 0;
}
