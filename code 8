#include <bits/stdc++.h>
using namespace std;
#define ll long long
#define for(n) for(i=0; i<n; i++)

void solve()
{
    ll int n,b,x,y,p,q,i,j,k,c=0,sum=0,mx = INT_MIN;
    cin>>n>>k;
    if(n%2==0){
        if(k-n<2)cout<<"-1"<<endl;
        else cout<<n<<" "<<n+2<<endl;
    }
    else{
        if(k-n<3)cout<<"-1"<<endl;
        else if(n%3==0) cout<<n<<" "<<n+3<<endl;
        else cout<<n+1<<" "<<n+3<<endl;
    }

}

int main()
{
    int t;
    cin>>t;
    while(t--)
    {
        solve();
    }
    return 0;
}
