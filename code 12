#include <iostream>
using namespace std;

int main() {
	// your code goes here
	int t;
	cin>>t;
	while(t--){
	    int x,y,z;
	    cin>>x>>y>>z;
	  long long int S=0,A=0;
	    while(x--){
	        string s;
	        cin>>s;
	        if(s[0]=='E' || s[0]=='Q' || s[0]=='U'|| s[0]=='I'|| s[0]=='N'|| s[0]=='O'|| s[0]=='X'){
	            S+=y;
	        }
	        else{
	            A+=z;
	        }
	    }
	    if(S>A){
	        cout<<"SARTHAK"<<endl;
	    }
	    else if(S==A){
	        cout<<"DRAW"<<endl;
	    }
	    else{
	        cout<<"ANURADHA"<<endl;
	    }
	    }
	return 0;
}
